
.uni-column {
	display: -webkit-box;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	        flex-direction: column;
}
.content {

	display: -webkit-box;

	display: flex;
	-webkit-box-flex: 1;
	        flex: 1;
	margin-bottom: 100rpx;
}
.foot {
	position: fixed;
	width: 100%;
	height: 90rpx;
	min-height: 90rpx;
	left: 0rpx;
	bottom: 0rpx;
	overflow: hidden;
}

