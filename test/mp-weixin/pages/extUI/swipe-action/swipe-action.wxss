
page {
	display: -webkit-box;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	        flex-direction: column;
	box-sizing: border-box;
	background-color: #fff
}
view {
	font-size: 28rpx;
	line-height: inherit
}
.example {
	padding: 0 30rpx 30rpx
}
.example-title {
	font-size: 32rpx;
	line-height: 32rpx;
	color: #777;
	margin: 40rpx 25rpx;
	position: relative
}
.example .example-title {
	margin: 40rpx 0
}
.example-body {
	padding: 0 40rpx
}
.cont {
	height: 90rpx;
	line-height: 90rpx;
	padding: 0 30rpx;
	position: relative;
}
.cont::before {
	position: absolute;
	z-index: 3;
	left: 0;
	right: 0;
	top: 0;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
	background-color: #c8c7cc;
}
.cont::after {
	position: absolute;
	z-index: 3;
	left: 0;
	right: 0;
	bottom: 0;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
	background-color: #c8c7cc;
}
.button-view {
	display: -webkit-box;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	        flex-direction: row;
	-webkit-box-pack: center;
	        justify-content: center;
	padding: 20rpx 0;
}
.button {
	border: 1px solid #E7E7E7;
	padding: 8rpx 16rpx;
	border-radius: 8rpx;
}

